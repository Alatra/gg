macro_previousoffice: macro_previousoffice{
compatible = "zmk,behavior-macro";
label = "macro_previousoffice";
#binding-cells = <0>;
bindings = <&kt LEFT_WIN>, <&kp LEFT_ARROW>;
};
macro_nextoffice: macro_nextoffice{
compatible = "zmk,behavior-macro";
label = "macro_nextoffice";
#binding-cells = <0>;
bindings = <&kt LEFT_CONTROL>, <&kt RIGHT_WIN>, <&kp RIGHT_ARROW>;
};